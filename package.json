  "main": "main.js",
  "scripts": {
    "fe-build": "run-s fe-clean fe-parcel",
    "fe-clean": "rimraf static",
    "fe-commit": "git add static",
    "fe-parcel": "cross-env NODE_ENV=development parcel build fe/index.html --out-dir static --public-url /static",
    "fe-watch": "parcel watch fe/index.html --out-dir static --public-url /static",
    "lint-staged": "lint-staged",
    "pre-commit": "run-s fe-build fe-commit lint-staged",
    "serve-dev": "ts-node-dev index.tsx",
    "start-dev": "run-p fe-watch serve-dev",
    "start": "ts-node index.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run-script pre-commit"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },

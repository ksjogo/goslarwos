{
  "name": "wos",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "fe-build": "run-s fe-clean fe-parcel",
    "fe-clean": "rimraf static",
    "fe-parcel": "parcel build fe/index.html --out-dir static --public-url /static",
    "fe-watch": "parcel watch fe/index.html --out-dir static --public-url /static",
    "serve-dev": "ts-node-dev index.tsx",
    "serve": "nodemon --exec 'ts-node' index.tsx",
    "start-dev": "run-p fe-watch serve-dev",
    "start": "run-s fe-build serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
